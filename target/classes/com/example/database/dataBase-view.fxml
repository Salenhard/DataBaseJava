<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="255.0" prefWidth="358.0" spacing="5.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/17" fx:controller="com.example.database.DataBaseController">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
   <HBox alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="337.0" spacing="5.0">
      <children>
         <Button fx:id="addObjectButton" mnemonicParsing="false" onAction="#clickAddOjbectButton" text="Add" />
         <Button mnemonicParsing="false" text="Clear" />
         <Button mnemonicParsing="false" text="Delete" />
         <Button fx:id="saveFileButton" mnemonicParsing="false" onAction="#clickSaveFileButton" text="Save file" />
         <Button fx:id="readFileButton" mnemonicParsing="false" onAction="#clickReadFileButton" text="Read file" />
      </children>
      <opaqueInsets>
         <Insets left="5.0" right="5.0" top="5.0" />
      </opaqueInsets>
      <VBox.margin>
         <Insets />
      </VBox.margin>
   </HBox>
   <ListView fx:id="objectsListView" editable="true" fixedCellSize="4.0" />
</VBox>
